<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
  </PropertyGroup>


  <PropertyGroup>
    <ProjectFolder>$[System.IO.Path]::GetFileName($(MSBuildThisFileDirectory))</ProjectFolder>
  </PropertyGroup>

  <Target Name="Diagnostic">
    <Message Text="ProjectFolder = $(ProjectFolder)" Importance="high" />
  </Target>

  <ItemGroup>
    <ProjectReference Include="$(Canonical)" />
    <ProjectReference Include="$(Collective)" />
    <ProjectReference Include="$(Components)" />
    <ProjectReference Include="$(Custom)" />
    <ProjectReference Include="$(Identify)" />  
    <ProjectReference Include="$(Monadic)" />
    <ProjectReference Include="$(Memories)" />    
    <ProjectReference Include="$(Reflections)" />
    <ProjectReference Include="$(SFuncs)" />    
    <ProjectReference Include="$(Symbolic)" />        
    <ProjectReference Include="$(Texting)" />
    <ProjectReference Include="$(Time)" />
  </ItemGroup>

</Project>
