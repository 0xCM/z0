<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
  </PropertyGroup>


  <PropertyGroup>
    <ProjectFolder>$[System.IO.Path]::GetFileName($(MSBuildThisFileDirectory))</ProjectFolder>
  </PropertyGroup>

  <Target Name="Diagnostic">
    <Message Text="ProjectFolder = $(ProjectFolder)" Importance="high" />
  </Target>

  <Import Project="$(Feeders)z.exports.props"/>

  <ItemGroup>
    <ProjectReference Include="$(Z)" />
  </ItemGroup>

</Project>
