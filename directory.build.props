<Project>

  <PropertyGroup>
    <TopDir>$(MSBuildThisFileDirectory)</TopDir>
    <PlatformTarget>x64</PlatformTarget>    
    <BuildRootDir>$(TopDir)bin\</BuildRootDir>
    <LibBuildDir>$(BuildRootDir)lib\</LibBuildDir>
    <OutputDir>$(LibBuildDir)</OutputDir>
    <OutputPath>$(LibBuildDir)</OutputPath>
    <DocumentationFile>$(LibBuildDir)$(MsBuildProjectName).xml</DocumentationFile>
    <IntermediateBuildDir>$(BuildRootDir)obj\$(MSBuildProjectName)\</IntermediateBuildDir>
    <BaseIntermediateOutputPath>$(IntermediateBuildDir)</BaseIntermediateOutputPath>
    <IntermediateOutputPath>$(IntermediateBuildDir)</IntermediateOutputPath>
    <NoWarn>1591;1572;1573;1701;1702;1712;1711;1570;1571;1574;1710;8321;NETSDK1057;0169;0414;0219</NoWarn>
    <NullableReferenceTypes>true</NullableReferenceTypes>
    <NullableContextOptions>disable</NullableContextOptions>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Optimize>true</Optimize>
    <DebugType>pdbonly</DebugType>    
    <Prefer32Bit>false</Prefer32Bit>
    <DebugSymbols>true</DebugSymbols>
    <LangVersion>preview</LangVersion>
    <SuppressNETCoreSdkPreviewMessage>true</SuppressNETCoreSdkPreviewMessage>
    <AssemblyVersion>1.0.41</AssemblyVersion>
    <VersionPrefix>$(AssemblyVersion)</VersionPrefix>
  </PropertyGroup>


  <PropertyGroup>
    <Root>$(TopDir)root/src/z0.root.csproj</Root>
    <Polyrand>$(TopDir)polyrand/src/z0.polyrand.csproj</Polyrand>
    <Nats>$(TopDir)nats/src/z0.nats.csproj</Nats>
    <VSvc>$(TopDir)vectorized/vsvc/z0.vectorized.vsvc.csproj</VSvc>
    <Symbolic>$(TopDir)symbolic/src/z0.symbolic.csproj</Symbolic>
    <TestApp>$(TopDir)testing/z0.testing.csproj</TestApp>
    <Math>$(TopDir)math/src/z0.math.csproj</Math>
    <GenericNumerics>$(TopDir)gmath/src/z0.gmath.csproj</GenericNumerics>
    <Blocks>$(TopDir)blocks/src/z0.blocks.csproj</Blocks>
    <Fixed>$(TopDir)fixed/src/z0.fixed.csproj</Fixed>
  </PropertyGroup>

</Project>


