<Project>

  <PropertyGroup>
    <TopDir>$(MSBuildThisFileDirectory)</TopDir>
    <PlatformTarget>x64</PlatformTarget>    
    <BuildRootDir>$(TopDir)bin\</BuildRootDir>
    <LibBuildDir>$(BuildRootDir)lib\</LibBuildDir>
    <OutputDir>$(LibBuildDir)</OutputDir>
    <OutputPath>$(LibBuildDir)</OutputPath>
    <DocumentationFile>$(LibBuildDir)$(MsBuildProjectName).xml</DocumentationFile>
    <IntermediateBuildDir>$(BuildRootDir)obj\$(MSBuildProjectName)\</IntermediateBuildDir>
    <BaseIntermediateOutputPath>$(IntermediateBuildDir)</BaseIntermediateOutputPath>
    <IntermediateOutputPath>$(IntermediateBuildDir)</IntermediateOutputPath>
    <NoWarn>1591;1572;1573;1701;1702;1712;1711;1570;1571;1574;1710;8321;NETSDK1057;0169;0414;0219</NoWarn>
    <NullableReferenceTypes>true</NullableReferenceTypes>
    <NullableContextOptions>disable</NullableContextOptions>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Optimize>true</Optimize>
    <DebugType>pdbonly</DebugType>    
    <Prefer32Bit>false</Prefer32Bit>
    <DebugSymbols>true</DebugSymbols>
    <LangVersion>preview</LangVersion>
    <SuppressNETCoreSdkPreviewMessage>true</SuppressNETCoreSdkPreviewMessage>
    <AssemblyVersion>1.0.41</AssemblyVersion>
    <VersionPrefix>$(AssemblyVersion)</VersionPrefix>
  </PropertyGroup>

  <PropertyGroup>
    <Src>$(TopDir)src/</Src>
    <SrcDir>$(TopDir)src/</SrcDir>
    <TestDir>$(TopDir)test/</TestDir>
  </PropertyGroup>

  <PropertyGroup>  
    <Parts>$(TopDir)parts/z0.parts.csproj</Parts>
  </PropertyGroup>

  <PropertyGroup>
    <Validity>$(TestDir)validity/z0.validity.csproj</Validity>
    <ValidityVectors>$(TestDir)vvalidity/z0.vvalidity.csproj</ValidityVectors>
  </PropertyGroup>

  <PropertyGroup>    
    <AsmTypes>$(TopDir)asm/types/z0.asm.types.csproj</AsmTypes>
    <AsmModels>$(TopDir)asm/models/z0.asm.models.csproj</AsmModels>
    <AsmCore>$(TopDir)asm/core/z0.asm.core.csproj</AsmCore>
    <AsmApp>$(TopDir)asm/app/z0.asm.app.csproj</AsmApp>
    <AsmDecode>$(TopDir)asm/decode/z0.asm.decode.csproj</AsmDecode>
    <AsmEncode>$(TopDir)asm/encode/z0.asm.encode.csproj</AsmEncode>
    <AsmSuite>$(TopDir)asm/src/z0.asm.csproj</AsmSuite>
  </PropertyGroup>

  <PropertyGroup>
    <BitCore>$(TopDir)bitcore/src/z0.bitcore.csproj</BitCore>
    <BitStrings>$(TopDir)bitstrings/src/z0.bitstrings.csproj</BitStrings>
    <BitPack>$(TopDir)bitpack/src/z0.bitpack.csproj</BitPack>
    <BitFields>$(TopDir)bitfields/src/z0.bitfields.csproj</BitFields>
    <BitVectors>$(TopDir)bitvectors/src/z0.bitvectors.csproj</BitVectors>
    <BitSvc>$(TopDir)bitsvc/src/z0.bitsvc.csproj</BitSvc>

    <VBits>$(TopDir)vbits/src/z0.vbits.csproj</VBits>
    <BitGrids>$(TopDir)bitgrids/src/z0.bitgrids.csproj</BitGrids>
    <BitSpans>$(TopDir)bitspans/src/z0.bitspans.csproj</BitSpans>
  </PropertyGroup>

  <PropertyGroup>
    <VSvc>$(TopDir)vsvc/src/z0.vsvc.csproj</VSvc>
    <FVec>$(TopDir)fvec/src/z0.fvec.csproj</FVec>
    <DVec>$(TopDir)dvec/src/z0.dvec.csproj</DVec>
    <GVec>$(TopDir)gvec/src/z0.gvec.csproj</GVec>    
    <VData>$(TopDir)vdata/src/z0.vdata.csproj</VData>
    <Circuits>$(TopDir)circuits/src/z0.circuits.csproj</Circuits>
  </PropertyGroup>
  
  <PropertyGroup>
    <Cil>$(TopDir)cil/src/z0.cil.csproj</Cil>
    <Containers>$(TopDir)containers/src/z0.containers.csproj</Containers>
    <Dynamic>$(TopDir)dynop/src/z0.dynop.csproj</Dynamic>
    <Identity>$(TopDir)identity/src/z0.identity.csproj</Identity>
    <LibM>$(TopDir)libm/src/z0.libm.csproj</LibM>
    <Logix>$(TopDir)logix/src/z0.logix.csproj</Logix>
    <Machines>$(TopDir)machines/src/z0.machines.csproj</Machines>
    <Matrix>$(TopDir)matrix/src/z0.matrix.csproj</Matrix>
    <MathSvc>$(TopDir)mathsvc/src/z0.mathsvc.csproj</MathSvc>
    <MklApi>$(TopDir)mkl/src/z0.mkl.csproj</MklApi>
    <Nats>$(TopDir)nats/src/z0.nats.csproj</Nats>
    <Polyrand>$(TopDir)polyrand/z0.polyrand.csproj</Polyrand>
    <Permute>$(TopDir)permute/src/z0.permute.csproj</Permute>
    <Stats>$(TopDir)stats/src/z0.stats.csproj</Stats>
  </PropertyGroup>

  <Import Project="$(SrcDir)core.export.props"/>


</Project>
