            <typeparam name="T">The reference type</typeparam>
            <remarks>
            u8:  movsxd rax,edx -&gt; add rax,rcx
            u16: movsxd rax,edx -&gt; lea rax,[rcx+rax*2]
            u32: movsxd rax,edx -&gt; lea rax,[rcx+rax*4]
            u64: movsxd rax,edx -&gt; lea rax,[rcx+rax*8]
            </remarks>
        
