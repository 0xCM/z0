<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>asmrun</AssemblyName>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
    <TieredCompilation>false</TieredCompilation>
    <TieredCompilationQuickJit>false</TieredCompilationQuickJit>
    <Deterministic>true</Deterministic>
    <Optimize>true</Optimize>
    <DebugType>pdbonly</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <NoWarn>1591;1572;1573;1701;1702;1712;1711;1570;1571;1574;1710;8321;NETSDK1057;0169;0414;0219;649;8632;SYSLIB0001;IDE0060;IDE0059;MSB3026</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="$(Asm)" />
    <ProjectReference Include="$(Capture)" />
    <ProjectReference Include="$(ResPack)" />
    <ProjectReference Include="$(Cli)" />
  </ItemGroup>

</Project>
